<Application x:Class="KubixAdmin.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:KubixAdmin"
             StartupUri="Windows/MainWindow.xaml">
    <Application.Resources>
        
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="Borders"  Source="pack://application:,,,/KubixAdmin;component/Resources/Borders.xaml" />
                <ResourceDictionary x:Name="Buttons"  Source="pack://application:,,,/KubixAdmin;component/Resources/Buttons.xaml" />
                <ResourceDictionary x:Name="Checkboxes"  Source="pack://application:,,,/KubixAdmin;component/Resources/Checkboxes.xaml" />
                <ResourceDictionary x:Name="CustomControls"  Source="pack://application:,,,/KubixAdmin;component/Resources/CustomControls.xaml" />
                <ResourceDictionary x:Name="TextBased"  Source="pack://application:,,,/KubixAdmin;component/Resources/TextBased.xaml" />
                <ResourceDictionary x:Name="ComboBoxes"  Source="pack://application:,,,/KubixAdmin;component/Resources/ComboBoxes.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
